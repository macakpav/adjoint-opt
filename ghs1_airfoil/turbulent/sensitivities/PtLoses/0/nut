FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform  0.005; 
// vysledek kOmegaSST na vstupu pro 0.62 Mach a 47 uhel je 0.005;

boundaryField
{
    PRESSURE-INLET
    {
        type            calculated;
        value 			$internalField;
    }

    PRESSURE-OUTLET
    {
        type            calculated;
        value 			$internalField;
    }

    PROF
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }

    "PERIOD.*"
    {
       type 			cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
