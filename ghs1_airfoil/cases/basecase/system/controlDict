FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         5000;

deltaT          1;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      2;

writeFormat     ascii;

writePrecision  16;

writeCompression false;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

functions
{
	#includeFunc "force"
	#includeFunc "outletAverage"
	#includeFunc "residualSaving"
}

// ************************************************************************* //
